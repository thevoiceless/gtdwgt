<% provide(:title, "Edit Profile") %>
<h1>Edit Your Profile</h1>

<div class="row">
	<div class="span6 offset3">
		<%= form_for(@user) do |f| %>
			<%= render 'fields', f: f %>
			<div class="center">
				<%= f.submit "Save Information", class: "btn btn-large btn-primary" %>
			</div>
		<% end %>
	</div>
</div>